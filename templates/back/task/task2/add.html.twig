{% extends 'back/index.html.twig' %}

{% block title %}Tâche P2 - Ajouter - Administration{% endblock %}

{% block body %}
	<div>
		{% block titre_admin_page %}Tâche P2 - Ajouter{% endblock %}
	</div>
	{% block admin_utilisateur %}
		<div class="was-validated form-back background-login is-valid">
			{% if notification %}
					<div class="alert alert-success">{{ notification }}<br>
						<hr>
						<a href="{{ path('task_list_admin') }}">Retourner à la liste</a>
					</div>
				<hr>
			{% endif %}
				{{ form_start(form_task_p2_add_admin) }}
			<div class="form-group">
				{{ form_widget(form_task_p2_add_admin) }}
			</div>
				{{ form_end(form_task_p2_add_admin) }}
		</div>
	{% endblock %}
{% endblock %}


