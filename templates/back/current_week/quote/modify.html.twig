{% extends 'back/admin.html.twig' %}

{% block title %}Modidier le devis de {{quote.subject}} - Semaine Actuelle - {% block title_base %}{{ parent() }}{% endblock %}{% endblock %}

{% block body %}
	<div>
		{% block title_back_page %}Modidier le devis de {{quote.subject}} - Semaine Actuelle{% endblock %}
	</div>
	{% block back_content %}
		<div class="was-validated form-back background-login is-valid">
			{% if notification %}
				<hr>
					<div class="alert alert-success">{{ notification }}<br>
						<hr>
						<a href="{{ path('list_cw_mission_back') }}">Retourner sur la liste</a>
					</div>
				<hr>
			{% endif %}
				{{ form_start(form_quote_cw_modify_back) }}
			<div class="form-group">
				{{ form_row(form_quote_cw_modify_back.subject) }}
				{{ form_row(form_quote_cw_modify_back.object) }}
				{{ form_row(form_quote_cw_modify_back.person) }}
				{{ form_row(form_quote_cw_modify_back.submit) }}
			</div>
				{{ form_end(form_quote_cw_modify_back) }}
	{% endblock %}
	</div>
{% endblock %}