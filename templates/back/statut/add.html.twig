{% extends 'back/index.html.twig' %}

{% block title %}Ajouter un statut - Administration{% endblock %}

{% block body %}
	<div>
		{% block titre_admin_page %}Statut - Ajouter{% endblock %}
	</div>

	{% block admin_utilisateur %}
		<div class="was-validated form-back background-login is-valid">
			{% if notification %}
					<div class="alert alert-success">{{ notification }}<br>
						<hr>
						<a href="{{ path('status_list_admin') }}">Retourner sur la liste des Statuts</a>
					</div>
				<hr>
			{% endif %}
				{{ form_start(form_admin_statut_add) }}
			<div class="form-group">
				{{ form_widget(form_admin_statut_add|raw) }}
			</div>
				{{ form_end(form_admin_statut_add) }}
		</div>
	{% endblock %}
{% endblock %}


