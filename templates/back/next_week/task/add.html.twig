{% extends 'back/index.html.twig' %}

{% block title %}Tâche - Ajouter - Semain Suivante -{% block title_base %}{{ parent() }}{% endblock %}{% endblock %}

{% block body %}
	<div>
{% block title_back_page %}Ajouter un tâche - Semaine Suivante{% endblock %}
	</div>
	{% block back_content %}
		<div class="was-validated form-back background-login is-valid">
		{% if notification %}
			<div class="alert alert-success">{{ notification }}<br>
				<hr>
				<a href="{{ path('mission_list_nw_back') }}">Retourner à la liste</a>
			</div>
		{% endif %}
			{{ form_start(form_task_nw_add_admin) }}
			<div class="form-group">
				{{ form_row(form_task_nw_add_admin.customer) }}
				{{ form_row(form_task_nw_add_admin.object) }}
				{{ form_row(form_task_nw_add_admin.subobject1) }}
				{{ form_row(form_task_nw_add_admin.subobject2) }}
				{{ form_row(form_task_nw_add_admin.subobject3) }}
				{{ form_row(form_task_nw_add_admin.users) }}
				{{ form_row(form_task_nw_add_admin.p1) }}
				{{ form_row(form_task_nw_add_admin.p2) }}
				{{ form_row(form_task_nw_add_admin.nextweek) }}
				{{ form_row(form_task_nw_add_admin.submit) }}
			</div>
		</div>
				{{ form_end(form_task_nw_add_admin) }}
	{% endblock %}
{% endblock %}


